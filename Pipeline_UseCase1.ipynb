    "from pykeen.predict import predict_target\n",
    "import pandas as pd\n",
    "\n",
    "# Convert IDs back to their string representations\n",
    "id_to_entity = {v: k for k, v in triples_factory.entity_to_id.items()}\n",
    "id_to_relation = {v: k for k, v in triples_factory.relation_to_id.items()}\n",
    "\n",
    "# Filter triples with the desired prefix and relation\n",
    "filtered_triples = [\n",
    "    (id_to_entity[triple[0]], id_to_relation[triple[1]], id_to_entity[triple[2]])\n",
    "    for triple in triples_factory.mapped_triples.tolist()\n",
    "    if id_to_entity[triple[0]].startswith(\"Diagnosis/\") and id_to_relation[triple[1]] == \"hasCode\"\n",
    "]\n",
    "\n",
    "# Evaluate predictions for all filtered triples\n",
    "correct_predictions = {\"Top 10\": 0, \"Top 5\": 0, \"Top 3\": 0, \"Top 1\": 0}\n",
    "total_triples = len(filtered_triples)\n",
    "\n",
    "for idx, (head, relation, true_tail) in enumerate(filtered_triples):\n",
    "    print(f\"Processing triple {idx + 1}/{total_triples}: {head}, {relation}, {true_tail}\")\n",
    "    \n",
    "    # Generate predictions for the current head and relation\n",
    "    predictions = predict_target(\n",
    "        model=result.model,\n",
    "        head=head,\n",
    "        relation=relation,\n",
    "        triples_factory=result.training\n",
    "    ).df\n",
    "\n",
    "    # Map tail IDs back to their string representations\n",
    "    predictions[\"tail_label\"] = predictions[\"tail_id\"].map(id_to_entity)\n",
    "\n",
    "    # Ensure consistent formatting for true_tail\n",
    "    normalized_true_tail = true_tail.strip()\n",
    "\n",
    "    # Sort predictions by score in descending order\n",
    "    predictions = predictions.sort_values(by=\"score\", ascending=False).reset_index(drop=True)\n",
    "\n",
    "    # Debugging: Show top 10 predictions and the true tail\n",
    "    print(predictions[[\"tail_label\", \"score\"]].head(10))\n",
    "    print(f\"True tail: {normalized_true_tail}\")\n",
    "\n",
    "    # Check if the true tail is in the top predictions\n",
    "    if normalized_true_tail in predictions[\"tail_label\"].values:\n",
    "        # Get the rank of the true tail\n",
    "        rank = predictions[predictions[\"tail_label\"] == normalized_true_tail].index[0] + 1\n",
    "        print(f\"Rank of true tail: {rank}\")\n",
    "        \n",
    "        # Update accuracy counters based on rank\n",
    "        if rank <= 10:\n",
    "            correct_predictions[\"Top 10\"] += 1\n",
    "        if rank <= 5:\n",
    "            correct_predictions[\"Top 5\"] += 1\n",
    "        if rank <= 3:\n",
    "            correct_predictions[\"Top 3\"] += 1\n",
    "        if rank == 1:\n",
    "            correct_predictions[\"Top 1\"] += 1\n",
    "    else:\n",
    "        print(f\"True tail '{normalized_true_tail}' not found in predictions for head: {head}, relation: {relation}\")\n",
    "\n",
    "# Calculate accuracies\n",
    "accuracies = {key: value / total_triples for key, value in correct_predictions.items()}\n",
    "\n",
    "# Display results\n",
    "print(\"Accuracy Results:\")\n",
    "print(f\"Top 10 Accuracy: {accuracies['Top 10']:.2f}\")\n",
    "print(f\"Top 5 Accuracy: {accuracies['Top 5']:.2f}\")\n",
    "print(f\"Top 3 Accuracy: {accuracies['Top 3']:.2f}\")\n",
    "print(f\"Top 1 Accuracy: {accuracies['Top 1']:.2f}\")\n",
    "\n",
    "# Save results for Overleaf\n",
    "results_df = pd.DataFrame.from_dict(accuracies, orient='index', columns=['Accuracy'])\n",
    "results_df.to_csv(\"embedding_accuracy_results.csv\")\n",
    "\n",
